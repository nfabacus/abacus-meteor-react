yout = ({content}) => (\n\t<MainLayoutWrapper content={content}/>\n)","import React, {Component} from 'react';\nimport NavButton from './NavButton.jsx';\nimport AccountsUI from '../AccountsUI.jsx';\nimport NavSideBar from './NavSideBar.jsx';\nimport MainContentSection from './MainContentSection.jsx';\n\n\nexport default class MainLayoutWrapper extends Component {\n\t\tconstructor(props, context) {\n\t\t\tsuper(props, context);\n\n\t\t\tthis.state = {\n\t\t\t\tnavButtonOn: false\n\t\t\t};\n\t\t}\n\n    onChildChanged(newState) {\n        this.setState({ navButtonOn: newState });\n    }\n    \n\n\trender(){\n\tconsole.log(this.state.navButtonOn);\n\n\t\treturn (\n\t\t\t<div className=\"main-layout\">\n\t\t\t\t<div id=\"wrapper\">\n\t\t\t\t\t<header id=\"nav_header\">\n\t\t\t\t        <span className=\"header_title\"><a href=\"/\">ABACUS LEARNING LAB</a></span>\n\t\t\t\t        <button id=\"toggle\" className=\"transparent-button smallBtn\">Start editing</button>\n\t\t\t\t        <button id=\"reset\" className=\"transparent-button smallBtn\">Reset content</button>\n\t\t\t\t        <button id=\"save\" className=\"transparent-button smallBtn\">Save content</button>\n\t\t\t\t        \n\t\t\t\t        {/* Nav Menu Hamburger Icon */}\n\t\t\t\t        <NavButton initialNavOn = {this.state.navButtonOn} callbackParent={this.onChildChanged}/>\n\n\t\t\t            {/* Horizontal Nav */}\n\t\t\t\t\t\t<nav className=\"horizontal_nav\">\n\t\t\t\t\t\t\t<a href=\"/\">Home</a>\n\t\t\t\t\t\t\t<a href=\"/resolutions\">Resolutions</a>\n\t\t\t\t\t\t\t<a href=\"/about\">About</a>\n\t\t\t\t\t\t\t<AccountsUI />\n\t\t\t\t\t\t</nav>\n\t\t\t\t\t</header>\n\n\t\t\t\t\t{/* Menu Sidebar */}\n\t\t\t\t\t<NavSideBar navSideBarOn = {this.state.navButtonOn} />\n\n\t\t\t\t\t{/* Main Content Section */}\n\t\t\t\t\t<MainContentSection navSideBarOn = {this.state.navButtonOn} content={this.props.content}/>\n\n\t\t\t      {/* Footer on the bottom of the page */}\n\t\t\t      <footer id=\"footer\" className=\"footerNoNav\">\n\t\t\t        <ul>\n\t\t\t          <li><a href=\"https://www.youtube.com/user/abacuslearning?feature=watch\" className=\"fa fa-youtube\"></a>\n\t\t\t          </li>\n\t\t\t          {/* <li><a href=\"#\" className=\"icon fa-instagram\"><span className=\"label\">Instagram</span></a></li> \n\t\t\t          <li><a href=\"#\" className=\"icon fa-dribbble\"><span className=\"label\">Dribbble</span></a></li> */}\n\t\t\t          <li>\n\t\t\t          <a href=\"https://www.facebook.com/Abacus-Maths-Learning-Course-211119545628882/timeline/\" className=\"fa fa-facebook\"></a>\n\t\t\t          </li>\n\t\t\t          <li><a href=\"https://plus.google.com/+AbacusmathsInfo/about\" className=\"fa fa-google-plus\"></a>\n\t\t\t          </li>\n\t\t\t          <li>\n\t\t\t          <a href=\"https://twitter.com/abacusmaths\" target=\"_blank\" className=\"fa fa-twitter\"></a>\n\t\t\t          </li>\n\t\t\t          <li>\n\t\t\t          <a href=\"https://www.linkedin.com/in/noby-fujioka-6741656\" target=\"_blank\" className=\"fa fa-linkedin\"></a>\n\t\t\t          </li>\n\t\t\t          <li>\n\t\t\t          <a href=\"mailto:contact@abacusmaths.info\" target=\"_blank\" className=\"fa fa-envelope-o\"></a>\n\t\t\t          </li>\n\t\t\t        </ul>\n\t\t\t        <ul>\n\t\t\t          <li className=\"copyright\">&copy; Abacus Learning Lab</li>\n\t\t\t        </ul>\n\t\t\t      </footer>\n\t\t\t\t</div>\n\n\t\t\t</div>\n\t\t)\n\t}\n}","import React, {Component} from 'react';\nimport TrackerReact from 'meteor/ultimatejs:tracker-react';\nimport ReactCSSTransitionGroup from 'react-addons-css-transition-group';\n\nexport default class NavButton extends Component {\n\t\tconstructor(props, context) {\n\t\t\tsuper(props, context);\n\n\t\t\tthis.state = {\n\t\t\t\tnavButtonOn: this.props.initialNavOn\n\t\t\t};\n\t\t}\n\n        _toggleNavSideBar(){\n        \tconsole.log('current state', this.state.navButtonOn);\n        \tvar newState = !this.state.navButtonOn;\n\n        \tthis.setState({\n            \tnavButtonOn: newState\n        \t});\n        \tthis.props.callbackParent(newState);\n        }        \n\n\trender() {\n\t\tif (this.state.navButtonOn) {\n\t\t\tvar menuButtonClass = \"\";\n\t\t} else {\n\t\t\tvar menuButtonClass = \"menu-button-off\";\n\t\t}\n\n\t\treturn (\n\n\t\t        <div id=\"menu-button\" className={menuButtonClass} onClick={this._toggleNavSideBar}>\n\t\t          <div id=\"line1\" className=\"hamburger-lines1\"></div>\n\t\t          <div id=\"line2\" className=\"hamburger-lines2\"></div>\n\t\t          <div id=\"line3\" className=\"hamburger-lines3\"></div>\n\t\t          <div id=\"line4\" className=\"hamburger-lines4\"></div>\n\t\t        </div>\n\n\t\t)\n\t}\n}","import React, {Component} from 'react';\nimport TrackerReact from 'meteor/ultimatejs:tracker-react';\nimport ReactCSSTransitionGroup from 'react-addons-css-transition-group';\n\nexport default class NavSideBar extends Component {\n\n\trender() {\n\t    if(this.props.navSideBarOn) {\n\t    \tnavSideBarClass = \"\";\n\t    } else {\n\t    \tnavSideBarClass = \"navHide\";\n\t    }\n\n\t\treturn (\n\n\t      <nav id=\"nav\" className={navSideBarClass}>         \n\t        <ul id=\"mainnav\">\n\t          <li><a href=\"/\">Home</a></li>\n\t          <li><a href=\"section_1\">Vision</a></li>\n\t          <li><a href=\"section_2\">Courses</a></li>\n\t          <li><a href=\"section_3\">News</a></li>\n\t          <li><a href=\"section_4\">Creativity</a></li>\n\t          <li><a href=\"section_5\">Contact</a></li>\n\t          <li><a href=\"parent_signup\">Sign Up</a></li> \n\t        </ul>\n\t     </nav>\n\n\t\t)\n\t}\n}","import React, {Component} from 'react';\nimport TrackerReact from 'meteor/ultimatejs:tracker-react';\n\nexport default class ResolutionDetail extends Component {\n\tconstructor() {\n\t\tsuper();\n\n\t\tthis.state = {\n\t\t\tsubscription: {\n\t\t\t\tresolutions: Meteor.subscribe(\"userResolutions\")\n\t\t\t}\n\t\t}\n\t}\n\n\tcomponentWillUnmount() {\n\t\tthis.state.subscription.resolutions.stop();\n\t}\n\n\tresolution() {\n\t\tconsole.log(Resolutions.findOne(this.props.id));\n\t\treturn Resolutions.findOne(this.props.id);\n\t}\n\n\trender() {\n\t\tlet res = this.resolution();\n\t\t\n\t\tif(!res) {\n\t\t\treturn(<div>Loading...</div>);\n\t\t}\n\t\treturn (\n\t\t\t<div>\n\t\t\t\t<h1>{res.text}</h1>\n\t\t\t</div>\n\t\t)\n\t}\n}","import React, {Component} from 'react';\n\nexport default class ResolutionSingle extends Component {\n\t\n\ttoggleChecked() {\n\t\tMeteor.call('toggleResolution', this.props.resolution);\n\t}\n\n\tdeleteResolution() {\n\t\tMeteor.call('deleteResolution', this.props.resolution);\n\t}\n\n\trender() {\n\t\tconst resolutionClass = this.props.resolution.complete ? \"checked\" : \"\";  //if complete is true, then add class \"checked\" to resolutionClass\n\t\tconst status = this.props.resolution.complete ? <span className =\"completed\">Completed</span> : \"\";\n\t\t\n\t\treturn (\n\t\t\t<li className={resolutionClass}>\n\t\t\t\t<input type=\"checkbox\"\n\t\t\t\t\treadOnly={true}\n\t\t\t\t\tchecked={this.props.resolution.complete}\n\t\t\t\t\tonClick={this.toggleChecked.bind(this)} />\n\t\t\t\t<a href={`/resolutions/${this.props.resolution._id}`}>{this.props.resolution.text}</a>\n\t\t\t\t{status}\n\t\t\t\t<button className=\"btn-cancel\"\n\t\t\t\t\tonClick={this.deleteResolution.bind(this)}>\n\t\t\t\t\t&times;\n\t\t\t\t</button>\n\n\t\t\t</li>\n\t\t)\n\t}\n}","import React, {Component} from 'react';\n\nexport default class ResolutionsForm extends Component {\n\taddResolution(event) {\n\t\tevent.preventDefault();\n\t\tvar text = this.refs.resolution.value.trim();\n\n\t\tMeteor.call('addResolution', text, (error, data)=>{\n\t\t\tif(error) {\n\t\t\t\tBert.alert('Please login before submitting', 'danger', 'fixed-top', 'fa-frown-o');\n\t\t\t} else {\n\t\t\t\tthis.refs.resolution.value = \"\";\n\t\t\t}\n\t\t\t\n\t\t});\t\n\t}\n\trender() {\n\t\treturn (\n\t\t\t<form className=\"new-resolution\" onSubmit={this.addResolution.bind(this)}>\n\t\t\t\t<input \n\t\t\t\t\ttype=\"text\" \n\t\t\t\t\tref=\"resolution\"\n\t\t\t\t\tplaceholder=\"Finish React Meteor Series\" />\n\t\t\t</form>\n\t\t)\n\t}\n}","import React from 'react';\nimport TrackerReact from 'meteor/ultimatejs:tracker-react';\nimport ReactCSSTransitionGroup from 'react-addons-css-transition-group';\n\nimport ResolutionsForm from './ResolutionsForm.jsx';\nimport ResolutionSingle from './ResolutionSingle.jsx';\n\nif(Meteor.isClient){\n\tResolutions = new Mongo.Collection(\"resolutions\");\n}\n\n\nexport default class ResolutionsWrapper extends TrackerReact(React.Component) {\n\tconstructor() {\n\t\tsuper();\n\n\t\tthis.state = {\n\t\t\tsubscription: {\n\t\t\t\tresolutions: Meteor.subscribe(\"userResolutions\")\n\t\t\t}\n\t\t}\n\t}\n\n\tcomponentWillUnmount() {\n\t\tthis.state.subscription.resolutions.stop();\n\t}\n\n\tresolutions() {\n\t\treturn Resolutions.find().fetch();\n\t}\n\n\trender() {\n\t\tDocHead.setTitle('My Resolutions | Abacus Learning Lab');\n\t\treturn (\n\t\t\t<ReactCSSTransitionGroup\n\t\t\t\tcomponent=\"div\"\n\t\t\t\ttransitionName=\"route\"\n\t\t\t\ttransitionEnterTimeout={600}\n\t\t\t\ttransitionAppearTimeout={600}\n\t\t\t\ttransitionLeaveTimeout={400}\n\t\t\t\ttransitionAppear={true}>\n\t\t\t\t<h1>My Resolutions</h1>\n\t\t\t\t<ResolutionsForm />\n\t\t\t\t<ReactCSSTransitionGroup\n\t\t\t\t\tcomponent=\"ul\"\n\t\t\t\t\tclassName=\"resolutions\"\n\t\t\t\t\ttransitionName=\"resolutionLoad\"\n\t\t\t\t\ttransitionEnterTimeout={600}\n\t\t\t\t\ttransitionLeaveTimeout={400}>\n\t\t\t\t\t{this.resolutions().map( (resolution)=>{\n\t\t\t\t\t\treturn <ResolutionSingle key={resolution._id} resolution={resolution} />\n\t\t\t\t\t})}\n\t\t\t\t</ReactCSSTransitionGroup>\n\t\t\t</ReactCSSTransitionGroup>\n\t\t)\n\t}\n}\n","import React, {Component} from 'react';\nimport ReactCSSTransitionGroup from 'react-addons-css-transition-group';\n\nexport default class About extends Component {\n\t\n\tsetV